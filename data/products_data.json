{
  "get_all_products_with_valid_token": {
    "expected_data": [
      {
        "id": 1,
        "name": "Book",
        "price": 10
      },
      {
        "id": 2,
        "name": "Pen",
        "price": 2
      }
    ],
    "status_code": 200
  },
  "get_all_products_without_token": {
    "expected_data": {
      "error": "Missing token"
    },
    "status_code": 401
  },
  "get_single_product_with_valid_id": {
    "id": 2,
    "expected_data": {
      "id": 2,
      "name": "Pen",
      "price": 2
    },
    "status_code": 200
  },
  "get_single_product_with_invalid_id": {
    "id": 99,
    "expected_data": {
      "error": "Product not found"
    },
    "status_code": 404
  },
  "create_product_with_admin_role": {
    "name": "Phone",
    "price": 500,
    "expected_data": {
      "id": 3,
      "name": "Phone",
      "price": 500
    },
    "status_code": 201
  },
  "create_product_with_user_role": {
    "name": "Phone",
    "price": 500,
    "expected_data": {
      "error": "Admin role required"
    },
    "status_code": 403
  },
  "update_product_with_admin_role": {
    "id": 1,
    "price": 500,
    "expected_data": {
      "id": 1,
      "name": "Book",
      "price": 500
    },
    "status_code": 200
  },
  "update_product_with_user_role": {
    "id": 1,
    "price": 500,
    "expected_data": {
      "error": "Admin role required"
    },
    "status_code": 403
  },
  "delete_product_with_admin_role": {
    "id": 2,
    "expected_data": {
      "message": "Product deleted"
    },
    "status_code": 200
  },
  "delete_product_with_user_role": {
    "id": 1,
    "expected_data": {
      "error": "Admin role required"
    },
    "status_code": 403
  }
}